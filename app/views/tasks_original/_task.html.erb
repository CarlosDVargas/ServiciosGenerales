<div id="<%= dom_id task %>">
  <p>
    <strong>Employee:</strong>
    <%= task.employee_id %>
  </p>

  <p>
    <strong>Request:</strong>
    <%= task.request_id %>
  </p>

  <p>
    <strong>Started at:</strong>
    <%= task.started_at %>
  </p>

  <p>
    <strong>Finished at:</strong>
    <%= task.finished_at %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= task.status %>
  </p>

</div>
